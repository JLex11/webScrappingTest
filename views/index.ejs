<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Offers Table of Scrapping</title>
  <link rel="stylesheet" href="static/styles.css">
</head>
<body>
  <div class="ContainerTable">
    <table>
      <tr>
        <th>*</th>
        <th>Title</th>
        <th>Location</th>
        <th>Link</th>
      </tr>
      <% for (var i = 0; i < offers.length; i++) { %>
        <tr>
          <td><%= i+1 %></td>
          <td><%= offers[i].title %></td>
          <td><%= offers[i].location %></td>
          <td><a href="<%= offers[i].link %>"><%= offers[i].link %></a></td>
        </tr>
      <% } %>
    </table>
  </div>
</body>
</html>